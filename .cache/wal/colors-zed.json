                    "constant": {
                      "color": "#E05D24"
                    },
                    "function": {
                      "color": "#D5952C"
                    },
                    "keyword": {
                      "color": "#E37226"
                    },
                    "number": {
                      "color": "#C3912C"
                    },
                    "operator": {
                      "color": "#E37226"
                    },
                    "string": {
                      "color": "#D95222"
                    },
                    "variable": {
                      "color": "#E05D24"
                    }
                },
                "players": [
                  {
                    "cursor": "#D95222",
